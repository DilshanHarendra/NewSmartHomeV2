<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nmadcreations.newsmarthomev2">

    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />

    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AdminDashBoard"></activity>
        <activity android:name=".GasController" />
        <activity android:name=".Agrement" />
        <activity android:name=".Login" />
        <activity android:name=".AddUserQr" />
        <activity android:name=".CreateSmartHome" />
        <activity android:name=".AlarmShedule" />
        <activity android:name=".ChangeColor" />
        <activity android:name=".ConnectToWifi" />
        <activity android:name=".BPSubMenu" />
        <activity android:name=".SubMenuList" />
        <activity android:name=".QrReader" />
        <activity
            android:name=".MainHome"
            android:label="@string/title_activity_main_home"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver android:name=".AlarmReciver" />
    </application>

</manifest>